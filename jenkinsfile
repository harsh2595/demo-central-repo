pipeline {
    agent {label "build_server"} #asdas
    
    stages {
        stage ("this stage will build the package") {
            steps {
                sh "mvn clean"
                sh "mvn install"
            }
        }
        stage ("this stage will copy package to s3 bucket") {
            steps {
                 sh "aws s3 cp /home/ubuntu/build_server/workspace/pipeline_example/target/hello-world-webapp.war s3://harsh-package"
            }

        }
    }
}
